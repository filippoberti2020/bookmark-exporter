{
  "manifest_version": 3,
  "name": "Bookmark Exporter",
  "version": "1.0",
  "permissions": [
    "bookmarks",
    "downloads"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png"
  }
}
